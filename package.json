{
  "name": "taskflare-expressjs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "docker compose up -d db && nodemon bin/www",
    "lint": "eslint ./**",
    "lint:fix": "eslint ./** --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "check": "npm run lint && npm run prettier",
    "format": "npm run lint:fix && npm run prettier:fix"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "http-errors": "~1.6.3",
    "jade": "^1.9.2",
    "mongoose": "^8.2.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5"
  }
}
